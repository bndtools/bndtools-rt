Private-Package: org.bndtools.rt.packager.mongodb.client
Conditional-Package: com.mongodb.*, org.bson.*
Service-Component: *

Import-Package: org.apache.felix.service.command;resolution:=optional, *

Bundle-Name: MongoDB Commands
Bundle-Description: This bundle connects to an instance of MongoDB advertised via the Endpoint service interface, and offers a set of GoGo commands in the scope "mongodb:" for manipulating the database.