Private-Package: org.bndtools.rt.packager.mosquitto.unix
Include-Resource: data=mosquitto/macos64

Require-Capability: packager.manager

Require-Capability:\
	osgi.native;filter:="(& \
		(osgi.native.osname=MacOSX) \
		(osgi.native.processor=x86-64) \
	)"
Provide-Capability: packager.type;packager.type=mosquitto;version:Version=1.0.2
Service-Component: *