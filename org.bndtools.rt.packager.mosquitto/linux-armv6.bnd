Private-Package: org.bndtools.rt.packager.mosquitto.unix,\
	org.bndtools.rt.packager.mosquitto.unix.linux
Include-Resource: data=mosquitto/linux_armv6

Require-Capability: \
	packager.manager;filter:="(!(version>=2.0))", \
	osgi.native;filter:="(& \
		(osgi.native.osname=Linux) \
		(osgi.native.processor=arm) \
	)"
Provide-Capability: packager.type;package.type=mosquitto;version:Version=1.0.2
Service-Component: *