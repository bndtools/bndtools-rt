	@${swt}!/org/*, \
	@${swt}!/*.so
Import-Package: !org.mozilla.xpcom, *
Require-Capability: osgi.native; filter:= " \
	(& \
		(osgi.native.osname=Linux) \
		(osgi.native.processor=x86) \
	)"

Bundle-Name: BrowserKit for Linux/Gtk x86